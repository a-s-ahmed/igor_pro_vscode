{
    "publisher": "qmsc",
    "name": "IgorPro",
    "displayName": "IgorPro",
    "description": "IgorPro Syntax highlighter",
    "version": "2.0.0",
    "repository": {
        "url": "https://github.lightsource.ca/ahmeda2/IgorPro8-VSCode"
    },
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "igor",
                "aliases": [
                    "Igor Pro",
                    "igor"
                ],
                "extensions": [
                    ".ipf"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "igor",
                "scopeName": "source.igor",
                "path": "./syntaxes/igorprobyte.tmLanguage"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^3.14.0"
    }
}
